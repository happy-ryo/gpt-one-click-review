{
  "name": "gpt-one-click-review",
  "displayName": "GPT One Click Review",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.81.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "gpt-one-click-review.reviewCode4",
        "title": "GPT-4 Review"
      },
      {
        "command": "gpt-one-click-review.reviewCode3",
        "title": "GPT-3 Review"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorHasSelection",
          "command": "gpt-one-click-review.reviewCode4",
          "group": "z_commands"
        },
        {
          "when": "editorHasSelection",
          "command": "gpt-one-click-review.reviewCode3",
          "group": "z_commands"
        }
      ]
    },
    "configuration": {
      "title": "GPT One Click Review",
      "properties": {
        "gpt-one-click-review.openaiApiKey": {
          "type": "string",
          "default": "",
          "description": "OpenAI API Key (sk-xxxxxxxxxxxxx)",
          "scope": "window"
        },
        "gpt-one-click-review.responseLanguage": {
                "type": "string",
                "default": "English",
                "enum": ["English", "Japanese", "Vietnamese"],
                "enumDescriptions": ["English", "日本語", "Tiếng Việt"],
                "description": "Select a language for the extension."
            }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.81.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "openai": "^4.0.0-beta.8"
  }
}